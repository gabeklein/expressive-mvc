{
  "testTimeout": 1000,
  "rootDir": "../",
  "roots": [
    "<rootDir>/packages/mvc",
    "<rootDir>/packages/react"
  ],
  "transform": {
    "^.+\\.[tj]sx?$": "@swc/jest"
  },
  "setupFilesAfterEnv": [
    "<rootDir>/jest.setup.ts"
  ],
  "moduleNameMapper": {
    "^@expressive/(.*)$": "<rootDir>/packages/$1/src"
  }
}