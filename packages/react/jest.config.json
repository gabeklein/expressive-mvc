{
  "testTimeout": 1000,
  "transform": {
    "^.+\\.[tj]sx?$": "@swc/jest"
  },
  "coveragePathIgnorePatterns": [
    "index.ts"
  ],
  "setupFilesAfterEnv": [
    "../../jest.setup.ts"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "moduleNameMapper": {
    "@expressive/(.*)$": "<rootDir>/../$1/src"
  }
}