{
  "preset": "ts-jest",
  "testTimeout": 1000,
  "coveragePathIgnorePatterns": [
    "/src/index.ts"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "moduleNameMapper": {
    "@expressive/(.*)$": "<rootDir>/../$1/src"
  }
}