{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "testTimeout": 1000,
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/src/index.ts",
    "tests/adapter.ts"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  }
}