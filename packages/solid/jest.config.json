{
  "testTimeout": 1000,
  "transform": {
    "^.+\\.[tj]sx?$": ["@swc/jest", {
      "jsx": {
        "factory": "solid-js/jsx-runtime",
        "pragma": "jsx"
      }
    }]
  },
  "coveragePathIgnorePatterns": [
    "index.ts"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "moduleNameMapper": {
    "@expressive/(.*)$": "<rootDir>/../$1/src"
  }
}